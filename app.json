{
  "expo": {
    "name": "CalistenIA",
    "slug": "calisteniaapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "android": {
      "package": "com.tuapp.calistenia",
      "permissions": [
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_DISTANCE",
        "android.permission.health.READ_TOTAL_CALORIES_BURNED",
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.tuapp.calistenia",
      "infoPlist": {
        "NSHealthShareUsageDescription": "CalistenIA necesita acceso a tus datos de salud para mostrarte tus pasos y actividad"
      }
    },
    "plugins": [
      "expo-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 26
          }
        }
      ]
    ]
  }
}